.bgr-image {
	position absolute
	top 0
	left 0
	background url($path-images + "bg1.png") center center no-repeat
	background-size cover
	width 100%
	height 100%
}

.bgr-mask {
	position absolute
	top -10px
	left -10px
	bottom -10px
	right -10px
	background url($path-images + "mask.svg") center center no-repeat
	background-size 100% 100%
}